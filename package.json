{
  "name": "@sprucelabs/spruce-calendar-utils",
  "publishConfig": {
    "access": "public"
  },
  "version": "9.6.255",
  "files": [
    "build/**/*",
    "!build/__tests__",
    "tsconfig.json"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "description": "Helpful calendar utilities",
  "skill": {
    "namespace": "calendar-utils",
    "upgradeIgnoreList": [],
    "commandOverrides": {
      "sync.schemas": "--shouldFetchCoreSchemas false --moduleToImportFromWhenRemote @sprucelabs/spruce-calendar-utils"
    }
  },
  "keywords": [
    "node",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/spruce-calendar-utils",
  "bugs": {
    "url": "https://github.com/sprucelabsai/spruce-calendar-utils/issues"
  },
  "scripts": {
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.dist": "yarn build.tsc && tsc --project tsconfig.dist.json && yarn build.resolve-paths && yarn build.esm-postbuild",
    "build.esm-postbuild": "esm-postbuild --target build/esm --patterns '**/*.js'",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "test": "jest",
    "update.dependencies": "yarn clean.dependencies && yarn",
    "upgrade.packages": "yarn-upgrade-all && rm -f yarn.lock ; yarn ; yarn fix.lint ; true",
    "upgrade.packages.all": "yarn install && yarn upgrade.packages",
    "upgrade.packages.test": "yarn upgrade.packages.all && yarn lint && yarn build.dev && yarn test",
    "watch.tests": "jest --watch",
    "watch.tsc": "tsc -w",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "lint.tsc": "tsc -p . --noEmit",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev"
  },
  "dependencies": {
    "@sprucelabs/schema": "^25.4.0",
    "@sprucelabs/spruce-core-schemas": "^22.6.148",
    "@sprucelabs/spruce-event-utils": "^17.1.131",
    "@sprucelabs/spruce-skill-utils": "^22.0.114",
    "date-fns": "^2.26.0",
    "rrule": "^2.6.8"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^1.0.286",
    "@sprucelabs/jest-json-reporter": "^6.0.245",
    "@sprucelabs/resolve-path-aliases": "^1.0.227",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/spruce-test-fixtures": "^14.0.3",
    "@sprucelabs/test": "^7.7.229",
    "@sprucelabs/test-utils": "^3.0.269",
    "@types/node": "^16.11.9",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-config-spruce": "^10.10.15",
    "jest": "^27.3.1",
    "jest-circus": "^27.3.1",
    "marked": "^4.0.4",
    "prettier": "^2.4.1",
    "timezone-mock": "^1.2.2",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "yarn-upgrade-all": "^0.5.4"
  },
  "jest": {
    "globalSetup": "<rootDir>/globalSetup.js",
    "testRunner": "jest-circus/runner",
    "maxWorkers": 4,
    "testTimeout": 120000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    }
  },
  "resolutions": {
    "marked": "2.0.7"
  }
}
