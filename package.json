{
  "name": "@sprucelabs/calendar-utils",
  "publishConfig": {
    "access": "public"
  },
  "version": "43.1.2",
  "files": [
    "build/**/*",
    "!build/__tests__",
    "tsconfig.json"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "license": "MIT",
  "description": "Helpful calendar utilities",
  "skill": {
    "namespace": "calendar-utils",
    "upgradeIgnoreList": [
      "@sprucelabs/spruce-skill-utils",
      "@sprucelabs/spruce-test-fixtures"
    ],
    "commandOverrides": {
      "sync.schemas": "--shouldFetchCoreSchemas false --moduleToImportFromWhenRemote @sprucelabs/calendar-utils"
    },
    "version": "v2021_05_19"
  },
  "keywords": [
    "node",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/calendar-utils",
  "bugs": {
    "url": "https://github.com/sprucelabsai/calendar-utils/issues"
  },
  "scripts": {
    "build.ci": "yarn run build.tsc && yarn run build.resolve-paths && yarn run lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn run build.tsc --sourceMap ; yarn run resolve-paths.lint",
    "build.dist": "tsc --project tsconfig.dist.json && yarn build.resolve-paths && mv build esm && yarn build.esm-postbuild && yarn build.tsc && yarn build.resolve-paths && mv esm build/ && yarn clean.dist",
    "build.esm-postbuild": "esm-postbuild --target esm --patterns '**/*.js'",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn run build.copy-files && tsc",
    "clean": "yarn run clean.build",
    "clean.all": "yarn run clean.dependencies && yarn run clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "clean.dist": "rm -rf build/__tests__ build/esm/__tests__",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "rebuild": "yarn run clean.all && yarn install && yarn run build.dev",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn run build.resolve-paths ; yarn run lint",
    "test": "jest",
    "update.dependencies": "yarn run clean.dependencies && yarn",
    "watch.tests": "jest --watch",
    "watch.tsc": "tsc -w",
    "post.watch.build": "yarn run build.copy-files && yarn run build.resolve-paths",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn run post.watch.build'",
    "lint.tsc": "tsc -p . --noEmit",
    "watch.rebuild": "yarn run clean.all && yarn install && yarn run watch.build.dev"
  },
  "dependencies": {
    "@sprucelabs/error": "^7.1.14",
    "@sprucelabs/mercury-event-emitter": "^43.0.123",
    "@sprucelabs/mercury-types": "^48.0.88",
    "@sprucelabs/schema": "^32.1.39",
    "@sprucelabs/spruce-core-schemas": "^41.1.3",
    "@sprucelabs/spruce-event-utils": "^41.0.100",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "rrule": "^2.8.1"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^7.0.31",
    "@sprucelabs/jest-json-reporter": "^9.0.49",
    "@sprucelabs/resolve-path-aliases": "^3.0.23",
    "@sprucelabs/semantic-release": "^5.0.2",
    "@sprucelabs/test": "^10.0.20",
    "@sprucelabs/test-utils": "^6.0.75",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.2",
    "chokidar-cli": "^3.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-config-spruce": "^11.2.26",
    "jest": "^30.1.3",
    "jest-circus": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "globalSetup": "<rootDir>/globalSetup.js",
    "testRunner": "jest-circus/runner",
    "maxWorkers": 4,
    "testTimeout": 120000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    }
  },
  "resolutions": {}
}
